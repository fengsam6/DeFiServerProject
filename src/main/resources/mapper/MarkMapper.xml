<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.cqupt.DeFiServerProject.mapper.MarkMapper">
    <select id="findAll" resultType="Mark">
        SELECT * FROM mark
    </select>

    <insert id="insert" parameterType="Mark">
        insert mark(`name`,exchange,getdate,hashcode)
        values (#{name}, #{exchange}, #{getdate}, #{hashcode})
    </insert>
</mapper>